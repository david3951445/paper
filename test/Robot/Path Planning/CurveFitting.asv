clc; clear; close all
r_dc = [
    0 1 1 0 1 2 3 4 5 5 4 4 3 2
    0 0 1 2 4 3 3 4 3 2 2 1 .5 0
];
r_dc = inter
t = linspace(0,1,length(r_dc));
x = r_dc(1, :);
y = r_dc(2, :);



options = fitoptions;
% options = fitoptions('Method','SmoothingSpline',...
%                      'SmoothingParam',1);
type = 'SmoothingSpline';
% type = 'cubicinterp';
% type = 'linearinterp';
f1 = fit(t', x', type, options);
f2 = fit(t', y', type, options);
t2 = linspace(0,1,length(r_dc)*10);
x2 = feval(f1,t2);
y2 = feval(f2,t2);

figure
plot(f1,t,x)
figure
plot(x,y)
hold on
plot(x2,y2)